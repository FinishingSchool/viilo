      <td>{{id}}</td>
      <td>{{name}}</td>
      <td>{{elo}}</td>
      <td>{{wins}}</td>
      <td>{{losses}}</td>
      <td>{{streak}}</td>
      <td>{{lastTen}}</td>
    </tr>
  {{/each}}
</table>

</body>
</html>